{
  "name": "notyourface",
  "private": false,
  "description": "Deterministically generated avatar images. Pretty, simple and lightweight.",
  "version": "1.0.5",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "module": "./dist/notyourface.js",
  "types": "./dist/notyourface.d.ts",
  "exports": {
    ".": {
      "types": "./dist/notyourface.d.ts",
      "import": "./dist/notyourface.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/bkis/notyourface/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkis/notyourface.git"
  },
  "keywords": [
    "avatars",
    "generate",
    "graphics",
    "images",
    "seed"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest run --coverage.enabled --coverage.thresholds.100",
    "build": "npm run fix && npm run test && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "fix": "run-s lint format",
    "prepare": "run-p fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "^7.1.7",
    "vitest": "^4.0.8",
    "vitest-canvas-mock": "^0.3.3"
  }
}
